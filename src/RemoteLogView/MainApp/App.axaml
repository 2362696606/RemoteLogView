    <!-- </Application.DataTemplates> -->
  
    <Application.Styles>
        <!-- <FluentTheme /> -->
        <themes:MaterialTheme PrimaryColor="Purple" SecondaryColor="Lime" BaseTheme="Light"/>
        <avalonia:MaterialIconStyles />
    </Application.Styles>
</Application>